<?xml version='1.0' encoding='UTF-8'?>
<data>
    <table name='users'>
        <column name='id_users'>bigserial PRIMARY KEY</column>
        <column name='login' data='owner'>varchar(255) NOT NULL UNIQUE</column>
    </table>
    <table name='hosts'>
        <column name='id_hosts'>bigserial PRIMARY KEY</column>
        <column name='hostname' data='host'>varchar(255) NOT NULL UNIQUE</column>
    </table>
    <table name='groupes'>
        <column name='id_groupes'>bigserial PRIMARY KEY</column>
        <column name='id_users' data='owner' datadb='login'>bigserial REFERENCES users(id_users)</column>
        <column name='groups_name' data='group'>varchar(255) NOT NULL UNIQUE</column>
    </table>
    <table name='metagroups'>
        <column name='id_metagroups'>bigserial PRIMARY KEY</column>
        <column name='id_groupes' data='group' datadb='groups_name'>bigserial REFERENCES groupes(id_groupes)</column>
        <column name='id_users' data='owner' datadb='login'>bigserial REFERENCES users(id_users)</column>
        <column name='metaname' data='host'>varchar(255) NOT NULL</column>
    </table>
    <table name='clusters'>
        <column name='id_clusters'>bigserial PRIMARY KEY</column>
        <column name='id_hosts' data='host' datadb='hostname'>bigserial REFERENCES hosts(id_hosts)</column>
        <column name='clusters_name' data='host'>varchar(255) NOT NULL</column>
    </table>  
    <table name='queue'>
        <column name='id_queue'>bigserial PRIMARY KEY</column>
        <column name='id_hosts' data='host' datadb='hostname'>bigserial REFERENCES hosts(id_hosts)</column>
        <column name='queue_name' data='qname'>varchar(255) NOT NULL UNIQUE</column> 
    </table>
    <table name='job_'>
        <column name='id_queue' data='qname' datadb='queue_name'>bigserial REFERENCES queue(id_queue)</column>
        <column name='id_hosts' data='host' datadb='hostname'>bigserial REFERENCES hosts(id_hosts)</column>
        <column name='id_groupes' data='group' datadb='groups_name'>bigserial REFERENCES groupes(id_groupes)</column>
        <column name='id_users' data='owner' datadb='login'>bigserial REFERENCES users(id_users)</column>
        <column name='job_name' data='job_name'>varchar(128) NOT NULL</column>
        <column name='job_id' data='job_id'>integer NOT NULL</column>
        <column name='submit_time' data='submit_time'>bigint NOT NULL</column>
        <column name='start_time' data='start'>bigint NOT NULL</column>
        <column name='end_time' data='end'>bigint NOT NULL</column>
        <column name='failed' data='fail'>integer</column>
        <column name='exit_status' data='exit_status'>integer</column>
        <column name='ru_wallclock' data='ru_wallclock'>real NOT NULL</column>
        <column name='ru_utime' data='ru_utime'>real NOT NULL</column>
        <column name='ru_stime' data='ru_stime'>real NOT NULL</column>
        <column name='project' data='project'>varchar(255)</column>
        <column name='slots' data='slots'>integer NOT NULL</column>
        <column name='cpu' data='cpu'>real NOT NULL</column>
        <column name='mem' data='mem'>real NOT NULL</column>
        <column name='io' data='io'>real NOT NULL</column>
        <column name='maxvmem' data='maxvmem'>real NOT NULL</column>
         <column name=''>PRIMARY KEY(id_queue, id_hosts, job_id, start_time, end_time)</column>
    </table>
</data>